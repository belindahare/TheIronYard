
<%= form_for @Patient do |f| %>

  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
  <br>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
  <br>
  <%= f.label :dob %>
  <%= f.text_field :dob %>
  <br>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <br>
  <%= f.label :gender %>
  <%= f.text_field :gender %>
  <br>

  <%= f.submit %>
  <%= link_to "Home", root_path %>

<% end %>